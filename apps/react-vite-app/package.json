{
  "name": "react-vite-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "yarn run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext js,cjs,ts,tsx --report-unused-disable-directives",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006 --no-open",
    "test-storybook": "test-storybook",
    "build-storybook": "storybook build",
    "build-storybook-docs": "storybook build --docs --output-dir storybook-docs-static",
    "clean": "rm -rf node_modules/.vite && rm -rf dist"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2",
    "@radix-ui/react-icons": "^1",
    "@radix-ui/react-slot": "^1",
    "@yarn-workspace-test/plus-four": "workspace:^",
    "@yarn-workspace-test/plus-two": "workspace:^",
    "class-variance-authority": "^0",
    "clsx": "^2",
    "lucide-react": "^0.316.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2",
    "tailwindcss-animate": "^1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.0.0-alpha.15",
    "@storybook/addon-interactions": "^8.0.0-alpha.15",
    "@storybook/addon-links": "^8.0.0-alpha.15",
    "@storybook/addon-themes": "^8.0.0-alpha.15",
    "@storybook/blocks": "^8.0.0-alpha.15",
    "@storybook/react": "^8.0.0-alpha.15",
    "@storybook/react-vite": "^8.0.0-alpha.15",
    "@storybook/test": "^8.0.0-alpha.15",
    "@storybook/test-runner": "^0",
    "@types/jest": "*",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4",
    "autoprefixer": "^10",
    "eslint": "*",
    "eslint-config-workspace": "workspace:^",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4",
    "eslint-plugin-react-refresh": "^0",
    "eslint-plugin-storybook": "^0",
    "jest": "*",
    "jest-config-workspace": "workspace:^",
    "postcss": "^8",
    "storybook": "^8.0.0-alpha.15",
    "tailwindcss": "^3",
    "tsconfig-workspace": "workspace:^",
    "typescript": "*",
    "vite": "^5"
  }
}
